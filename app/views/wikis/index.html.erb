<h1>Wikis</h1>
<%= link_to "New", new_wiki_path(@wiki), class: 'btn btn-sq-sm btn-success' %>
<% @wikis.each do |wiki| %>
<div class="media">
    <div class="media-body">
        <div class="media-heading">
            <p><%= link_to wiki.title, wiki %><% if wiki.private == true %> <strong> -private </strong> <% end %></p>
        </div>
    </div>
</div>
 <% end %>
